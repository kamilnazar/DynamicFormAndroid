{
  "form-name": "User Details",
  "fields": [
    {
      "field-name": "name",
      "type": "text",
      "required": false
    },
    {
      "field-name": "details",
      "type": "composite",
      "fields": [
        {
          "field-name": "age",
          "type": "composite",
          "fields": [
            {
              "field-name": "age1",
              "type": "number"
            },
            {
              "field-name": "age2",
              "type": "number"
            }
          ]
        },
        {
          "field-name": "gender",
          "type": "dropdown",
          "options": [
            "male",
            "female",
            "other"
          ]
        },
        {
          "field-name": "address",
          "type": "composite",
          "fields": [
            {
              "field-name": "address-line-1",
              "type": "text"
            },
            {
              "field-name": "street-address",
              "type": "composite",
              "fields": [
                {
                  "field-name": "street",
                  "type": "text"
                },
                {
                  "field-name": "buildings",
                  "type": "text"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}


